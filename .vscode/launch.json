	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'sonic1'",
			"stdio": [
				null,
				null, // "${workspaceFolder}/sonic1/out.txt", // null,
				"${workspaceFolder}/sonic1/err.txt",
			],
			"cargo": {
				"args": [
					"build",
					"--manifest-path",
					"sonic1/Cargo.toml",
					"--bin=sonic1",
					"--package=sonic1"
				],
				"filter": {
					"name": "sonic1",
					"kind": "bin"
				}
			},
			"args": [
				// "--test",
			],
			"cwd": "${workspaceFolder}/sonic1",
			"env": {
				"RUST_BACKTRACE": "1",
			},
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'asmconverter'",
			"cargo": {
				"args": [
					"build",
					"--manifest-path",
					"asmconverter/Cargo.toml",
					"--bin=asmconverter",
					"--package=asmconverter"
				],
				"filter": {
					"name": "asmconverter",
					"kind": "bin"
				}
			},
			"args": [
				// "--test",
			],
			"cwd": "${workspaceFolder}/asmconverter",
			"env": {
				"RUST_BACKTRACE": "1",
			},
		},
	]
}